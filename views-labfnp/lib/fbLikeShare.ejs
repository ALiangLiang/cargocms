<% if(req.url === "/") { %>
  <iframe src="https://www.facebook.com/plugins/like.php?href=https%3A%2F%2Fwww.facebook.com%2FLabFnP&width=142&layout=button_count&action=like&size=large&show_faces=false&share=true&height=46&appId=1011584022234802" width="142" height="46" style="border:none;overflow:hidden" scrolling="no" frameborder="0" allowTransparency="true"></iframe>
<% } else { %>
  <% var shareUrl = socialData.url; %>
  <% shareUrl = shareUrl.replace(/\/|\:/g, function(match){ %>
    <% if(match === ":") { %>
    <%   return "%3A"; %>
    <% } else if(match === "/"){ %>
    <%   return "%2F"; %>
    <% } %>
  <% }); %>

  <iframe src="https://www.facebook.com/plugins/like.php?href=<%= shareUrl %>&width=142&layout=button_count&action=like&size=large&show_faces=false&share=true&height=46&appId=1011584022234802" width="142" height="46" style="border:none;overflow:hidden" scrolling="no" frameborder="0" allowTransparency="true"></iframe>
<% } %>
