
<%- include form.ejs %>

<!-- SCRIPTS ON PAGE EVENT -->
<script type="text/javascript">


	var pagefunction = function() {
		<%- include vue.ejs %>
    appMain.loadSupllier();
		appMain.$mount("#main-edit");
		appMain.loadItem( function(){
      appMain.onEnterPageEdit();
      appMain.supplierRole();
      appMain.addressColumn();

      if (appMain.data.item.deletedAt) {
        appMain.hideUserDisabledBtn();
        appMain.userEnabled();
      } else {
        appMain.userDisabled();
      }
    });


		$('label.user').removeClass('state-disabled');
		$('input[name="user"]').removeAttr('chekced');
		$('input[name="user"]').removeAttr('disabled');

		appMain.setupForm("edit");
    appMain.DateOfBirthday();
    
	};

</script>
