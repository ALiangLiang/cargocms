<!-- Meta -->
<meta charset="utf-8">
<meta name="viewport" content="width=device-width, initial-scale=1.0, width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0">
<!-- <meta name="author" content=""> -->
<meta name="description" content="於自動門界服務已超過10年以上資歷，安裝過台灣前500大型企業內-包括了無塵室自動門包括聯電、 台積電、日月光、奇美電子、矽品、友達等不勝枚舉…，歡迎參觀比較。">

<!-- FB OG tags -->
<meta property="og:site_name" content="捷陞專業自動門" />
<meta property="og:locale" content="zh_TW" />
<meta property="og:url" content="<%= SocialService.getShareUrl() + req.url %>" />
<meta name="fb:app_id" content="<%= sails.config.passport.facebook.options.clientID %>">

<!-- other Meta tags -->
<% LayoutUtils.getMetas().forEach(function(meta) { %>
<meta property="<%= meta.property %>" content="<%= meta.content %>" /> <% }); %>
